<%- include('header'); -%>


<div class="container px-4">
    <h1>Dashboard</h1>
    <div>
      <p>Last heartbeat: <%= lastHeartbeat %></p>
      <p>Stale: <%= stale %></p>
    </div>
    <table class="table">
        <thead>
          <tr>
            <th scope="col">#</th>
            <th scope="col">Title</th>
            <th scope="col">Path</th>
          </tr>
        </thead>
        <tbody>
            <% sketches.forEach(function(sketch) { %>
                <tr>
                    <th scope="row"><%= sketch.id %></th>
                    <td><%= sketch.title %></td>
                    <td><%= sketch.directory %></td>
                </tr>
            <% }); %> 
        </tbody>
      </table>
</div>

<%- include('footer'); -%>